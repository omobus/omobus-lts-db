<?xml version="1.0" encoding="utf-8"?>
<contacts templ="%contact_id%;%account_id%;%name%;%surname%;%patronymic%;%job_title_id%;%spec_id%;%cohort_id%;%loyalty_level_id%;%influence_level_id%;%intensity_level_id%;%start_year%;%mobile%;%email%;%locked%;%extra_info%;%consent_status%;%consent_data%;%consent_type%;%consent_dt%;%author_id%;%hidden%;%cookie%;">
<verification>
  <![CDATA[
select exist_data_stream2('%uid%', '%pack_code%', '%pack_digest%') exist
  ]]>
</verification>
<check_exist>
  <![CDATA[
select count(*) exist from contacts 
    where db_id = '%ErpCode%' and contact_id = '%contact_id%'
  ]]>
</check_exist>
<update>
  <![CDATA[
update contacts set account_id='%account_id%', name='%name%', surname=descr_in('%surname%'), patronymic=descr_in('%patronymic%'), job_title_id='%job_title_id%', spec_id=uid_in('%spec_id%'), cohort_id=uid_in('%cohort_id%'), loyalty_level_id=uid_in('%loyalty_level_id%'), influence_level_id=uid_in('%influence_level_id%'), intensity_level_id=uid_in('%intensity_level_id%'), mobile=phone_in('%mobile%'), email=email_in('%email%'), locked=0%locked%, extra_info=note_in('%extra_info%'), consent_status=uid_in('%consent_status%'), consent_data=resolve_blob_stream2('%uid%', '%consent_data%'), consent_type=uid_in('%consent_type%'), consent_dt=datetime_in('%consent_dt%'), author_id=uid_in('%author_id%'), hidden=0%hidden%, cookie=uid_in('%cookie%')
    where db_id = '%ErpCode%' and contact_id = '%contact_id%'
  ]]>
</update>
<insert>
  <![CDATA[
insert into contacts (db_id, contact_id, account_id, name, surname, patronymic, job_title_id, spec_id, cohort_id, loyalty_level_id, influence_level_id, intensity_level_id, mobile, email, locked, extra_info, consent_status, consent_data, consent_type, consent_dt, author_id, hidden, cookie) 
    values ('%ErpCode%', '%contact_id%', '%account_id%', '%name%', descr_in('%surname%'), descr_in('%patronymic%'), '%job_title_id%', uid_in('%spec_id%'), uid_in('%cohort_id%'), uid_in('%loyalty_level_id%'), uid_in('%influence_level_id%'), uid_in('%intensity_level_id%'), phone_t('%mobile%'), email_t('%email%'), 0%locked%, note_in('%extra_info%'), uid_in('%consent_status%'), resolve_blob_stream2('%uid%', '%consent_data%'), uid_in('%consent_type%'), datetime_in('%consent_dt%'), uid_in('%author_id%'), 0%hidden%, uid_in('%cookie%'))
  ]]>
</insert>
<end>
  <![CDATA[
select stor_data_stream2('%uid%', '%pack_code%', '%pack_digest%', '%server_hostname%')
  ]]>
