<?xml version="1.0" encoding="utf-8"?>
<asp_types templ="%asp_type_id%;%descr%;%row_no%;">
<verification>
  <![CDATA[
select count(*) from data_stream
    where s_id = '//proxy/%uid%/%pack_code%' and digest = '%pack_digest%'
  ]]>
</verification>
<begin>
  <![CDATA[
update asp_types set hidden=1
    where db_id = '%ErpCode%' and hidden = 0
  ]]>
</begin>
<check_exist>
  <![CDATA[
select count(*) exist from asp_types 
    where db_id = '%ErpCode%' and asp_type_id = '%asp_type_id%'
  ]]>
</check_exist>
<update>
  <![CDATA[
update asp_types set descr='%descr%', row_no=int32_in('%row_no%'), hidden=0
    where db_id = '%ErpCode%' and asp_type_id = '%asp_type_id%'
  ]]>
</update>
<insert>
  <![CDATA[
insert into asp_types (db_id, asp_type_id, descr, row_no, hidden) 
    values ('%ErpCode%', '%asp_type_id%', '%descr%', int32_in('%row_no%'), 0)
  ]]>
</insert>
<end>
  <![CDATA[
#ifdef PGSQL
select stor_data_stream('//proxy/%uid%/%pack_code%', '%pack_digest%', '%server_hostname%')
#endif //PGSQL
#ifdef MSSQL
exec stor_data_stream '//proxy/%uid%/%pack_code%', '%pack_digest%', '%server_hostname%'
#endif //MSSQL
  ]]>
</end>
</asp_types>